{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": "ec2:*",
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "elasticloadbalancing:*",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "cloudwatch:*",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "autoscaling:*",
      "Resource": "*"
    },
    {
      "Effect": "Deny",
      "Action": "aws-portal:*Billing",
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "kms:Create*",
        "kms:Enable*",
        "kms:Get*",
        "kms:Put*"
      ],
      "Resource": "*"
    }
  ]
}
